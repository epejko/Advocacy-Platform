
<html>
<head>
    <title>Login</title>
</head>
<body>
    <div>
    <%= form_for(:session, url: login_path) do |f| %>
        <%= f.label :email, "Email" %>
        <%= f.email_field :email %>

        <%= f.label :password, "Password" %>
        <%= f.password_field :password, class: 'form-control' %>

        <%= f.submit "Log In" %>
    <% end %>
    </div>
</body>
</html>