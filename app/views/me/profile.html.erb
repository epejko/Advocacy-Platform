<html>
<head>
    <title>Profile</title>
</head>
<body>
    <% if current_user %>
        <% current_user.attributes.each do |key, value| %>
            <p><%= key %>: <%= value %></p> <br>
        <% end %>
        <p>My Tasks:</p>
        <% @tasks.each do |t| %>
            <p><%= t.subject %></p>
        <% end %>
    <% end %>
</body>
</html>